{
    "jsc": {
        "target": "es5",
        "parser": {
            "syntax": "typescript",
            "tsx": true,
            "decorators": false,
            "dynamicImport": true
        },
        "minify": {}
    },
    "env": {
        "targets": {
            "ie": "11"
        },
        "corejs": "3"
    },
    "module": {
        "type": "commonjs",
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "sourceMaps": true,
    "minify": true
}